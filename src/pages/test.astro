---
import Layout from "@/layouts/Layout.astro";
import LayoutU from "@/layouts/LayoutU.astro";
import VarianRadio from "@components/blocks/VarianRadio.astro";
import Button from "@components/shared/Button.astro";
import answer1 from "@assets/images/answers/answer-1.png";
import answer2 from "@assets/images/answers/answer-2.png";
import answer3 from "@assets/images/answers/answer-3.png";
import answer4 from "@assets/images/answers/answer-4.png";
import answer5 from "@assets/images/answers/answer-5.png";
import answer6 from "@assets/images/answers/answer-6.png";
import answer7 from "@assets/images/answers/answer-7.png";
import answer8 from "@assets/images/answers/answer-8.png";
import question1 from "@assets/images/answers/question-1.png";
import question2 from "@assets/images/answers/question-2.png";
import Pagination from "@components/shared/Pagination.astro";
