                </VarianRadio>
                <VarianRadio id="answer-6-2" name="answer-6" value="value-2">
                  120
                </VarianRadio>
                <VarianRadio id="answer-6-3" name="answer-6" value="value-3">
                  80
                </VarianRadio>
                <VarianRadio id="answer-6-4" name="answer-6" value="value-4">
