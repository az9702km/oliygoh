import Card from "@components/shared/Card.astro";

const { expanded } = Astro.props
---

<div class="row posts-grid">
  <div class="col-1">
    <Card
      imgsrc={blog1.src}
      bodyUp
      titleClass="!h-[5.25rem] !line-clamp-3">
      <Fragment slot="title">
        Xalqaro Nordik universiteti haqida nimalar bilishimiz muhim
      </Fragment>
      <Fragment slot="subtitle">ABITURENTLAR</Fragment>
    </Card>
  </div>
  <div class="col-1">
    <Card
      imgsrc={blog2.src}
      bodyUp
      titleClass="!h-[5.25rem] !line-clamp-3">
      <Fragment slot="title">
        O‘zbekistondagi top-5 universitetlar 
      </Fragment>
      <Fragment slot="subtitle">Darslar</Fragment>
    </Card>
  </div>
  <div class="col-1">
    <Card
      imgsrc={blog3.src}
      bodyUp
      titleClass="!h-[5.25rem] !line-clamp-3">
      <Fragment slot="title">
        Alegbra fanida yangi darslar boshlandi
      </Fragment>
      <Fragment slot="subtitle">o‘qituvchi</Fragment>
    </Card>
  </div>
  <div class="col-1">
    <Card
      imgsrc={blog4.src}
      bodyUp
      titleClass="!h-[5.25rem] !line-clamp-3">
      <Fragment slot="title">
        Tarix fanini 12 yilik tajribaga ega o‘qtuvchidan o‘rganing
      </Fragment>
      <Fragment slot="subtitle">o‘qituvchi</Fragment>
