          <Fragment slot="subtitle">ABITURENTLAR</Fragment>
        </Card>
      </div>
      <div class="col-1">
        <Card
          imgsrc={blog17.src}
          bodyUp
          titleClass="!h-[5.25rem] !line-clamp-3">
          <Fragment slot="title">
