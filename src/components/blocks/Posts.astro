import blog6 from "@assets/images/blog/blog-6.png";
import blog7 from "@assets/images/blog/blog-7.png";
import blog8 from "@assets/images/blog/blog-8.png";
import blog9 from "@assets/images/blog/blog-9.png";
import blog10 from "@assets/images/blog/blog-10.png";
